                "download_detour": "direct"
            },
            {
                "tag": "cn-ips",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
                "download_detour": "proxy"
            }
        ],
        "rules": [
            {
                "port": 53,
                "action": "hijack-dns"
            },
            {
                "rule_set": "block",
                "action": "reject"
            },
            {
                "rule_set": [
                    "direct",
                    "steam",
                    "cn-ips"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "telegram",
                    "proxy"
                ],
                "outbound": "proxy"
            }
        ],
        "final": "final",
        "auto_detect_interface": true
