{
    "log": {
        "level": "warn"
    },
    "dns": {
        "servers": [
            {
                "tag": "ali-dns",
                "address": "https://223.5.5.5/dns-query",
                "detour": "direct-out"
            },
            {
                "tag": "fakeip",
                "address": "fakeip"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "ali-dns"
            },
            {
                "rule_set": "block",
                "action": "reject"
            },
            {
                "query_type": "HTTPS",
                "rule_set": "proxy",
                "action": "reject"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "rule_set": "proxy",
                "server": "fakeip"
            }
        ],
        "final": "ali-dns",
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        }
    },
